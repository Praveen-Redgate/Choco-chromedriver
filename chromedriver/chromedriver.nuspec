<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>chromedriver</id>
    <title>Chromedriver</title>
    <version>2.21</version>
    <authors>C.Small</authors>
    <owners>C.Small</owners>
    <summary>Downloads and installs the latest version of Chromedriver.exe from http://chromedriver.storage.googleapis.com</summary>
    <description>Developed in collaboration with the Chromium team, ChromeDriver is a standalone server which implements WebDriver's wire protocol.This package finds the latest version of Chromedriver.exe from http://chromedriver.storage.googleapis.com, downloads it and install it.</description>
    <projectUrl>https://sites.google.com/a/chromium.org/chromedriver/</projectUrl>
    <packageSourceUrl>https://github.com/yetanotherchris/Choco-chromedriver-latest</packageSourceUrl>
    <tags>chromedriver webdriver chrome</tags>
    <copyright>Chromedriver authors</copyright>
    <licenseUrl>http://unlicense.org</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>http://cdn.rawgit.com/yetanotherchris/Choco-chromedriver-latest/master/icon.png</iconUrl>
    <releaseNotes>Moved to version number matching that on the chromedriver official site (for upgrades).</releaseNotes>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
